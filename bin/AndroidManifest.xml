<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.interq"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
	<uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.interq.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.interq.QuestionHomeActivity"
            android:label="Question Topics"
            android:screenOrientation="portrait" >
        </activity>
        <activity 
            android:name="com.example.interq.ReviewHomeActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity 
            android:name="com.example.interq.ForumHomeActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity 
            android:name="com.example.interq.SettingsHomeActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity 
            android:name="com.example.interq.ReviewTopicActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity 
            android:name="com.example.interq.QuestionAskingActivity"
            android:screenOrientation="portrait"
            >
        </activity>
        
        
    </application>

</manifest>